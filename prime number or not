//WAP to find entered number is prime number or not 
#include<stdio.h>
int main()
{	int n;
	printf("Enter a number ");
	scanf("%d",&n);
	 
	int i;
	for(i=2; i<n; i++)
	if(n % i == 0){
		printf("it is not a prime number ");
		break;
    }
	if(i+1==n){
		printf("it is a prime number");
	  }
	return 0;
} 
