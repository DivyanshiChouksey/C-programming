//WAP to see print function
#include<stdio.h>
int main()
{
	printf("hello \nworld\n");   		  //\n new line
	printf("hello \t world");	        //\t one tab place
	printf("\nhello \\ world\n");		  //\\to print '\'
	printf("\'hello\' \'world\'\n");	//\' to print'
	printf("hello \f world\n");       //\f to print symbol
	printf("hello \a world\n");			  //\a alert sound
	printf("hello\bworld\n");			    //\b last char remove
	printf("hello \rab\n");				    //\r to replace
	return 0;
}
