//WAP to insert new element at the end of given array

#include<stdio.h>
int main()
{	int num[50] = {};
	int n;
	printf("enter the size of array you want\n");
	scanf("%d",&n);
	
	for (int i=0; i<n; i++){	
	scanf("%d",&num[i]);
	}
	printf("\nbefore inserting new element\n");
	for(int i=0; i<n; i++){
		printf("%d ",num[i]);
	}
	printf("\nenter a new element ");
	scanf("%d",&num[n]);
	printf("after inserting new element \n");
	for(int i=0; i<=n; i++){
		printf("%d ",num[i]);
	}
	return 0;
}
